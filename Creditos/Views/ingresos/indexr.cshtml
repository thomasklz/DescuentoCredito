            },
            success: function (data) {
                console.log(data);
                if (data.validar == true) {
                    $("#contIngresos").html(data.tabla);
                } else {
                    $("#contIngresos").html(data.mensaje);
                }
            },
            complete: function () {
            },
            error: function (xhr, textStatus, errorThrown) {

                if (xhr.status === 0) {
                    $("#contIngresos").html('<div class="alert alert-danger text-center" role="alert">NO HAY CONEXIÓN A INTERNET. VERIFICA LA RED</div>');
                } else if (xhr.status == 404) {
